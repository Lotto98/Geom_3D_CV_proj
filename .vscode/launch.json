        {
            "name": "Python Debugger: calibation",
            "type": "debugpy",
            "request": "launch",
            "program": "src/calibration.py",
            "console": "integratedTerminal",
            "args": [
                "--name",
                "moving_light"
            ]
        },
        {
            "name": "Python: analysis --reglighting",
            "type": "debugpy",
            "request": "launch",
